generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model ticks {
  id        String       @id @default(uuid())
  tradeId   BigInt          
  asset     String
  price     String
  time      BigInt
  quantity  String
}

model OneMinTicks {
  id        String      @id @default(uuid())
  tradeId   BigInt  
  asset     String
  open      String
  high      String
  low       String
  close     String
  openTime  BigInt
  closeTime BigInt
}

model FiveMinTicks {
  id        String      @id @default(uuid())
  tradeId   BigInt  
  asset     String
  open      String
  high      String
  low       String
  close     String
  openTime  BigInt
  closeTime BigInt
}

model TenMinTicks {
  id        String      @id @default(uuid())
  tradeId   BigInt  
  asset     String
  open      String
  high      String
  low       String
  close     String
  openTime  BigInt
  closeTime BigInt
}

model ThirtyMinTicks {
  id        String      @id @default(uuid())
  tradeId   BigInt  
  asset     String
  open      String
  high      String
  low       String
  close     String
